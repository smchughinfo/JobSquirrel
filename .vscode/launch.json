    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Stashboard",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\AcornDepot\\Stashboard\\server.js",
            "preLaunchTask": "build-stashboard-frontend"
        },
        {
            "name": "Build Stashboard Frontend",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}\\AcornDepot\\Stashboard\\node_modules\\npm\\bin\\npm-cli.js",
            "args": ["run", "build"],
            "cwd": "${workspaceFolder}\\AcornDepot\\Stashboard",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}